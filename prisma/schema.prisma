generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // puedes usar tambi√©n "mysql" o "sqlite"
  url      = env("DATABASE_URL")
}

model Contact {
  id        Int      @id @default(autoincrement())
  email     String
  subject   String
  message   String
  createdAt DateTime @default(now())
}